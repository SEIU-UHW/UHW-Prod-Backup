        <externalId>false</externalId>
        <formula>if (left(text(Transfer_Type__c), 1) = &quot;A&quot;, &quot;CHECK-IN&quot;,
  if (text(Transferred_From__r.Account.Account_Group__c)=&apos;Staff&apos;,
    if(left(Transferred_From__r.Department,  3) = &apos;OPS&apos;,
       UPPER(&apos;Operations&apos;),
