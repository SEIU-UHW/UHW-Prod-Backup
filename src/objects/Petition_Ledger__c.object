    <validationRules>
        <fullName>County_Assignment</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  OR(left(text(Transfer_Type__c), 1) = &quot;A&quot;, 
     left(text(Transfer_Type__c), 1) = &quot;O&quot; 
  ),
  text(County__c) = &quot;No County Assigned&quot;
)</errorConditionFormula>
        <errorMessage>Check-ins and audit records must have a valid county assigned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Have_From</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
  left(text(Transfer_Type__c), 1) != &quot;P&quot;, 
  isblank( Transferred_From__c )
)</errorConditionFormula>
        <errorMessage>After printing, all Petition Ledger records must have a To and a From person.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_To_For_Burn_and_Audit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  OR(left(text(Transfer_Type__c), 1) = &quot;A&quot;,
     left(text(Transfer_Type__c), 1) = &quot;X&quot;
  ),
  NOT(ISBLANK( Transferred_To__c))
)</errorConditionFormula>
        <errorMessage>Do not put anyone in Transfer To for audits and burns.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_Not_Zero</fullName>
        <active>true</active>
        <errorConditionFormula>Quantity__c &lt; 1</errorConditionFormula>
        <errorMessage>Quantity must be 1 petition or greater.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Signatures_for_CheckIn_and_Audit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(left(text(Transfer_Type__c), 1) != &quot;A&quot;,
      left(text(Transfer_Type__c), 1) != &quot;O&quot;,
       OR(Valid_Signatures__c &gt; 0,
          Bad_Signatures__c &gt; 0)
  )</errorConditionFormula>
        <errorMessage>Valid and bad signature counts should only be on check-ins and audit records.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
