    <fields>
        <fullName>Daily_Overtime_Hours__c</fullName>
        <description>Over time hours</description>
        <externalId>false</externalId>
        <formula>IF(AND( 
Time_Difference__c == (Daily_Hours_Spent__c+ Time_Off_Total__c + Paid_Break_Total__c + Unpaid_Lunch_Total__c ), AND($User.Hourly_Employee__c, $User.Non_Exempt_Employee__c), AND(Daily_Hours_Total__c !=0 , Daily_Hours_Total__c &gt; 8) 
), Daily_Hours_Total__c - 8, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Total Paid Hours - 8 hours to caluclaute Over time hours</inlineHelpText>
        <label>Daily Overtime Hours</label>
