        function processStaff(results) {
            let i,id,name,division,title,contId,contId2,records,rec;
            let batCnt = 0;
            let processedAll = false;

            debugger;
            while (!processedAll) {
                records = results.getArray('records');
                if (records.length) {
                    for (i = 0; i < records.length; i++) {
                        rec      = records[i];
                        id       = rec.Id;
                        name     = rec.FirstName + ' ' + rec.LastName;
                        division = rec.Division;
                        title    = rec.Title;
                        contId   = rec.Staff_Contact_Id__c;
                        contId2  = contId.substring(1,15);

                        if (!staffMap[contId2])
                            staffMap[contId2] = {Id:id,Name:name,Division:division,Title:title};
                    }
                    if (results.done == 'true')
                        processedAll = true;
                    else {
                        batCnt++;
                        results = sforce.connection.queryMore(results.queryLocator);
                    }
                }
                else processedAll = true;
            }
        }

        function loadStaff() {
            let staffQuery = "SELECT Division,FirstName,LastName,Title,Id,Staff_Contact_Id__c FROM User WHERE IsActive = true AND Staff_Contact_Id__c != null ORDER BY LastName ASC NULLS LAST, FirstName ASC NULLS LAST";
            console.log(staffQuery);
            try {
                repeatMessageDiv.innerHTML = 'Retrieving Users ... ';
                let userResults = sforce.connection.query(staffQuery);
                processStaff(userResults);
            }
            catch(e) {
                alert('An error occurred retrieving Users: ' + e.faultstring);
                noError = false;
            }
        }

        function loadMembers() {
            let memberQuery = "SELECT Recruit__r.Id, Location__c,Recruit__r.FirstName,Recruit__r.LastName,Recruit__r.HomePhone,Recruit__r.MobilePhone, Credit__c " +
                    "          FROM Recruit__c Where Attended__c = true And Recruit__r.Id != Null And Credit__c In ('Full','Half')  And Volunteer_Date__c >= 2016-09-19 " +
                    "          Order By Location__c,Recruit__r.LastName,Recruit__r.FirstName,Credit__c ";
            try {
                repeatMessageDiv.innerHTML = 'Retrieving Member counts ... ';
                let memberResults = sforce.connection.query(memberQuery);
                processMembers(memberResults);
            }
            catch (e) {
                alert('An error occurred retrieving Members: ' + e.faultstring);
                noEr = false;
            }
        }

        function loadNonMembers() {
            let nonMemberQuery = "SELECT Id,Location__c,Alt_First_Name__c,Alt_Last_Name__c,Alt_Phone__c,Credit__c " +
                    "             FROM Recruit__c Where Attended__c = true And Recruit__r.Id = Null And Credit__c In ('Full','Half') And Volunteer_Date__c >= 2016-09-19 " +
                    "             Order By Location__c,Alt_Last_Name__c,Alt_First_Name__c,Credit__c ";
            console.log(nonMemberQuery);
            try {
                repeatMessageDiv.innerHTML = 'Retrieving Non-Member counts ... ';
                let nonMemberResults = sforce.connection.query(nonMemberQuery);
                processNonMembers(nonMemberResults);
            }
            catch(e) {
                alert('An error occurred retrieving Non Members: ' + e.faultstring);
                noError = false;
            }
        }

        function processMembers(results) {
            let id,id2,i,rec,staffRec,firstName,lastName,name,location,credit;
            let records;
            let batCnt = 0, staffCnt = 0;
            let processedAll = false;
            let homePhone,cellPhone;

            memberCount = 0;
            while (!processedAll) {
                records = results.getArray('records');
                console.log(' [Retrieved : ' + records.length + ' member records (batch: ' + batCnt + ')]');
                if (records.length) {
                    memberCount = records.length;
                    for (i = 0; i < records.length; i++) {
                        ptr++;
                        firstName = lastName = '';
                        rec       = records[i];
                        id        = rec.Recruit__r.Id;
                        id2 = id.substring(1,15);

                        if(staffMap[id2]) {
                            staffRec = staffMap[id2];
                            console.log('Skipping staffer attendance - ' + staffRec.Name + ' | ' + staffRec.Division + ' | ' + staffRec.Title);
                            staffCnt++;
                        }
                        else {
                            location = rec.Location__c;
                            credit = rec.Credit__c;
                            firstName = toProperCase(rec.Recruit__r.FirstName);
                            lastName = toProperCase(rec.Recruit__r.LastName);
                            homePhone = phoneFormat(rec.Recruit__r.HomePhone);
                            cellPhone = phoneFormat(rec.Recruit__r.MobilePhone);
                            name = lastName + ', ' + firstName;

                            if (!locationNameMap[location])
                                locationNameMap[location] = {};

                            if (!locationNameMap[location][name])
                                locationNameMap[location][name] = {
                                    Name: name,
                                    CellPhone: cellPhone,
                                    HomePhone: homePhone
                                };

                            if (!locNameCreditMap[location])
                                locNameCreditMap[location] = {};

                            if (!locNameCreditMap[location][name])
                                locNameCreditMap[location][name] = {};

                            if (!locNameCreditMap[location][name][credit])
                                locNameCreditMap[location][name][credit] = {
                                    Location: location,
                                    Name: name,
                                    Member: true,
                                    Count: 1
                                };
                            else
                                locNameCreditMap[location][name][credit].Count++;
                        }
                    }
                    if (results.done == 'true')
                        processedAll = true;
                    else {
                        batCnt++;
                        results = sforce.connection.queryMore(results.queryLocator);
                    }
                }
                else processedAll = true;
            }
        }

        function processNonMembers(results) {
            let i,id,rec,firstName,lastName,name,location,credit;
            let records;
            let batCnt = 0;
            let processedAll = false;
            let homePhone,cellPhone;

            homePhone = '';
