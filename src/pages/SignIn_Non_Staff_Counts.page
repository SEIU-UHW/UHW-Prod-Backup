                {headerName: 'Half Credit', field: 'HalfCredit',  width:  90, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1' },
                {headerName: 'Cell Phone',  field: 'CellPhone',   width: 150, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1' },
                {headerName: 'Home Phone',  field: 'HomePhone',   width: 150, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1' }
            ];

            statsColumnDefs = [
                {headerName: 'Location',        field: 'Location',              width: 150, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass3' },
                {headerName: 'People Count',    field: 'LocCnt',                width: 120, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1' },
                {headerName: 'Repeat Member',   field: 'RepeatMemberCount',     width: 150, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1' },
                {headerName: '%',               field: 'RepeatMemberCountPC',   width:  90, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1', valueGetter: repeatMemberCountPcValueGetter, cellRenderer:repeatMemberCountCellRenderer},
                {headerName: 'Single Member',   field: 'SingleMemberCount',     width: 150, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1' },
                {headerName: '%',               field: 'SingleMemberCountPC',   width:  90, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1', valueGetter: singleMemberCountPcValueGetter, cellRenderer:singleMemberCountCellRenderer},
                {headerName: 'Repeat NonMember',field: 'RepeatNonMemberCount',  width: 150, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1' },
                {headerName: '%',               field: 'RepeatNonMemberCountPC',width:  90, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1', valueGetter: repeatNonMemberCountPcValueGetter, cellRenderer:repeatNonMemberCountCellRenderer},
                {headerName: 'Single NonMember',field: 'SingleNonMemberCount',  width: 150, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1' },
                {headerName: '%',               field: 'SingleNonMemberCountPC',width:  90, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1', valueGetter: singleNonMemberCountPcValueGetter, cellRenderer:singleNonMemberCountCellRenderer},
                {headerName: 'Total Repeat %',  width:  150, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1', valueGetter: totalRepeatPcValueGetter, cellRenderer:totalRepeatPcCellRenderer},
                {headerName: 'Total Single %',  width:  150, suppressMenu: true, sortingOrder: ['asc','desc'], cellClass: 'cellClass1', valueGetter: totalSinglePcValueGetter, cellRenderer:totalSinglePcCellRenderer}
            ];
        }

        function showResults() {
            repeatCount = repeatMemberCount + repeatNonMemberCount;
            if(repeatCount == 1)
                repeatMessageDiv.innerHTML = 'Found 1 person';
            else {
                if (locCnt == 1)
                    repeatMessageDiv.innerHTML = 'Found ' + repeatCount + ' people in 1 location';
                else
                    repeatMessageDiv.innerHTML = 'Found ' + repeatCount + ' people in ' + locCnt + ' locations';
            }

            singleCount = singleMemberCount + singleNonMemberCount;
            if(singleCount == 1)
                singleMessageDiv.innerHTML = '(1 person attended once)';
            else if(singleCount > 1)
                singleMessageDiv.innerHTML = '(' + singleCount + ' people attended once)';

            showGrid();
        }

        function summarize() {
            let locNameCreditMapKey,nameCreditMap,nameCreditMapKey,creditMap,creditKey;
            let loc,mbr,cnt;
            let statLocCnt,statRepeatMemberCount,statRepeatNonMemberCount,statSingleMemberCount,statSingleNonMemberCount;
            let fullCreditCount,halfCreditCount,name,homePhone,cellPhone;

            repeatMemberCount    = 0;
            repeatNonMemberCount = 0;
            singleMemberCount    = 0;
            singleNonMemberCount = 0;
            locCnt               = 0;

            statsDataArray = [];

            for (locNameCreditMapKey in locNameCreditMap) {
                statLocCnt  = statRepeatMemberCount = statRepeatNonMemberCount = statSingleMemberCount = statSingleNonMemberCount = 0;
                locCnt++;
                loc = locNameCreditMapKey;

                repeatMessageDiv.innerHTML = 'Building Summary' + loc;
                nameCreditMap = locNameCreditMap[locNameCreditMapKey];
                for (nameCreditMapKey in nameCreditMap) {
                    statLocCnt++;
                    name = nameCreditMapKey;
                    creditMap = nameCreditMap[nameCreditMapKey];
                    fullCreditCount = halfCreditCount = 0;

                    homePhone = locationNameMap[loc][name].HomePhone;
                    cellPhone = locationNameMap[loc][name].CellPhone;

                    for (creditKey in creditMap) {
                        mbr = creditMap[creditKey].Member;
                        cnt = creditMap[creditKey].Count;
                        if (creditKey.toLowerCase() == 'full')
                            fullCreditCount = cnt;
                        else
                            halfCreditCount = cnt;
                    }

                    if (fullCreditCount + halfCreditCount > 1) {
                        if (mbr) {
                            repeatMemberCount++;
                            statRepeatMemberCount++;
                        }
                        else {
                            repeatNonMemberCount++;
                            statRepeatNonMemberCount++;
                        }

                        repeatCountDataArray.push({
                            Location: loc,
                            Name: name,
                            CellPhone: cellPhone,
                            HomePhone: homePhone,
                            Member: mbr,
                            FullCredit: fullCreditCount,
                            HalfCredit: halfCreditCount
                        });
                    }
                    else {
                        if (mbr) {
                            singleMemberCount++;
                            statSingleMemberCount++;
                        }
                        else {
                            singleNonMemberCount++;
                            statSingleNonMemberCount++;
                        }

                        singleCountDataArray.push({
                            Location: loc,
                            Name: name,
                            CellPhone: cellPhone,
                            HomePhone: homePhone,
                            Member: mbr,
                            FullCredit: fullCreditCount,
                            HalfCredit: halfCreditCount
                        });

                    }
                }
                statsDataArray.push({Location:loc, LocCnt:statLocCnt, RepeatMemberCount:statRepeatMemberCount,SingleMemberCount:statSingleMemberCount, RepeatNonMemberCount:statRepeatNonMemberCount, SingleNonMemberCount:statSingleNonMemberCount});
            }
        }

        function showGrid() {

            if(shownBefore) {
                repeatCountGridOptions.api.destroy();
                singleCountGridOptions.api.destroy();
                statsGridOptions.api.destroy();
            }
            else shownBefore = true;

            repeatCountGridOptions = {
                columnDefs: repeatCountColumnDefs,
                rowData: repeatCountDataArray,
                rowSelection: 'single',
                enableSorting: true
            };
            new agGrid.Grid(repeatCountGridDiv, repeatCountGridOptions);

            singleCountGridOptions = {
                columnDefs: singleCountColumnDefs,
                rowData: singleCountDataArray,
                rowSelection: 'single',
                enableSorting: true
            };
            new agGrid.Grid(singleCountGridDiv, singleCountGridOptions);

            statsGridOptions = {
                columnDefs: statsColumnDefs,
                rowData: statsDataArray,
                rowSelection: 'single',
                enableSorting: true
            };
            new agGrid.Grid(statsGridDiv, statsGridOptions);

        }

        function dateChangeHandler(dVal,dStr) {

            ddv = dStr;
            ptr = 0;
            repeatCount          = 0;
            repeatCountDataArray = [];
            singleCountDataArray = [];
            locNameCreditMap     = {};
            locationNameMap      = {};
            locationArray        = [];

            loadMembers();
            loadNonMembers();
            if(noError) {
                if (memberCount + nonMemberCount > 0) {
                    setControls('visible');
                    summarize();
                    showResults();
                }
                else {
                    setControls('hidden');
                    repeatMessageDiv.innerHTML = 'No records found with that start date';
                    singleMessageDiv.innerHTML = '';
                    if(shownBefore) {
                        repeatCountGridOptions.api.destroy();
                        singleCountGridOptions.api.destroy();
                        statsGridOptions.api.destroy();
                        shownBefore = false;
                    }
                }
            }
        }

        function setControls(pVisible) {
            var visStyle = 'visibility:' + pVisible ;
            saveAsCsvButtonR.setAttribute("style",visStyle);
            saveAsCsvButtonS.setAttribute("style",visStyle);
            saveAsCsvButtonStats.setAttribute("style",visStyle);
            desc1.setAttribute("style",visStyle);
            desc2.setAttribute("style",visStyle);
            desc3.setAttribute("style",visStyle);
        }

        function init() {

            sforce.connection.sessionId = "{!$Api.Session_ID}";

            repeatMessageDiv     = document.getElementById('RepeatMessageDiv');
            repeatCountGridDiv   = document.getElementById('RepeatCountGridDiv');
            singleMessageDiv     = document.getElementById('SingleMessageDiv');
            singleCountGridDiv   = document.getElementById('SingleCountGridDiv');
            statsGridDiv         = document.getElementById('StatsGridDiv');
            saveAsCsvButtonR     = document.getElementById("SaveAsCsvButtonR");
            saveAsCsvButtonS     = document.getElementById("SaveAsCsvButtonS");
            saveAsCsvButtonStats = document.getElementById("SaveAsCsvButtonStats");

            repeatMessageDiv.innerHTML = '';
            saveAsCsvButtonR.addEventListener("click", saveAsCsvR, false);
            saveAsCsvButtonS.addEventListener("click", saveAsCsvS, false);
            saveAsCsvButtonStats.addEventListener("click", saveAsCsvStats, false);

            desc1 = document.getElementById('Desc1');
            desc2 = document.getElementById('Desc2');
            desc3 = document.getElementById('Desc3');

            repeatCount          = 0;
            repeatCountDataArray = [];
            singleCountDataArray = [];
            locNameCreditMap     = {};
            locationNameMap      = {};
            locationArray        = [];
            staffMap             = {};

            setControls('hidden');

            var datePicker = document.getElementById('DatePicker');
            picker = new Pikaday({
                field: datePicker,
                format: 'L',
                onSelect: function() {
                    let dv = this.getMoment().format('L');
                    let ds = this.getMoment().format('YYYY-MM-DD');
                    picker.hide();
                    dateChangeHandler(dv, ds);
                }
            });
        }

        function saveAsCsvR() {
            let fileName = 'Repeat Sign-In counts (starting: ' + ddv + ').csv';
            repeatCountGridOptions.api.exportDataAsCsv({fileName: fileName});
        }

        function saveAsCsvS() {
            let fileName = 'Single Sign-In counts (starting: ' + ddv + ').csv';
            singleCountGridOptions.api.exportDataAsCsv({fileName: fileName});
        }

        function saveAsCsvStats() {
            let fileName = 'Single Sign-In Statistics (starting: ' + ddv + ').csv';
            statsGridOptions.api.exportDataAsCsv({fileName: fileName});
        }

        function showLoadingMsg(show) {
            if(show) // modal window fake
                modalLoadingDiv.setAttribute('class', 'modalDialog');
            else
                modalLoadingDiv.setAttribute('class', 'no-show');
        }

        function phoneFormat(s) {
            let s1;
            if(s == null || s.trim().length < 10)
                s1 = '';
            else {
                s1 = s.trim().replace(/[^0-9]/g,'');
                s1 = '(' + s1.substr(0,3) + ') ' + s1.substr(3,3) + '-' + s1.substr(6);
            }
            return s1;
        }

        function toProperCase(s) {
            // My version - handles hyphenated names correctly :)
            let s1 = '';
            if(s)
                s1 = s.trim().toLowerCase().replace(/\b[a-zA-Z]/g,function($1) {return $1.toUpperCase();});

            return s1;
        }

        function dateFmt(dStr) {
            let localDate;
            yyyy      = dStr.substr(0,4);
            mm        = dStr.substr(5,2);
            dd        = dStr.substr(8,2);
            localDate = mm + '/' + dd + '/' + yyyy;
            return localDate;
        }

        function dateRenderer(params) {
            keyDate   = params.data.Date;
            return dateFmt(keyDate);
        }

        var locNameCreditMap,locationNameMap;
        var locationArray;
        var statsColumnDefs;
        var statsDataArray;
        var repeatCountDataArray;
        var singleCountDataArray;
        var repeatCountColumnDefs;
        var singleCountColumnDefs;
        var repeatCountGridOptions;
        var singleCountGridOptions;
        var statsGridOptions;
        var picker;
        var repeatMessageDiv, repeatCountGridDiv;
        var singleMessageDiv, singleCountGridDiv;
        var repeatCount,repeatMemberCount,repeatNonMemberCount,singleMemberCount,singleNonMemberCount,locCnt;
        var shownBefore;
        var ptr;
        var saveAsCsvButtonR,saveAsCsvButtonS,saveAsCsvButtonStats;

        var ddv, yyyy,mm,dd;
        var showModal = false;
        var noError = true;
        var memberCount,nonMemberCount;
        var desc1,desc2,desc3;
        var staffMap;

        // ***************** Main Program Entry *****************
        init();
        initAgGrid();

        loadStaff();

        ddv = '2016-09-19';
        loadMembers();
        loadNonMembers();
        setControls('visible');
        summarize();
        showResults();

    </script>
    </body>
