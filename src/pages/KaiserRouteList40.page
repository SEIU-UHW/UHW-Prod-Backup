          }

          if (org1Choice == 1) {
            mainDataArray[ptr].Organizer1 = 'Vacant';
            mainDataArray[ptr].Org1Id = '';
          }
          if (org1Choice > 1) {
            mainDataArray[ptr].Organizer1 = org1Record.Name;
            mainDataArray[ptr].Org1Id = org1Record.Id;
          }

          if (org2Choice == 1) {
            mainDataArray[ptr].Organizer2 = '';
