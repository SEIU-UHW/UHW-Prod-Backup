          }
          if (org2Choice > 1) {
            mainDataArray[ptr].Organizer2 = org2Record.Name;
            mainDataArray[ptr].Org2Id = org2Record.Id;
          }

          if (csChoice == 1 && ptr == selectedRow) {
            mainDataArray[ptr].ContractSpecialist = '';
