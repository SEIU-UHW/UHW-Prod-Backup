    function formatFacilityOrAddrress(pStr) {

      var facLen = pStr.length;
      var lastChar = pStr.substr(facLen - 1, 1);
      if (lastChar == '|')
        pStr = pStr.substr(0, facLen - 1);

      facStr = pStr.trim();
      var pipe = / \|/g;
      var facStrF = facStr.replace(pipe, ',');
      return facStrF;
    }

