    public static void toProperCase(List<Contact> pNewList) {
      System.debug('[toProperCase] start');
      for (Contact c: pNewList) {
        if (c.LastName != null && c.LastName.length() > 2 && (c.LastName.toLowerCase().trim().equals(c.LastName.trim()) || c.LastName.toUpperCase().trim().equals(c.LastName.trim()))) {
          c.LastName = Report_Custom_Utils.toProperCase(c.LastName);
        }
        if (c.MiddleName != null && c.LastName.length() > 2 && (c.MiddleName.toLowerCase().trim().equals(c.MiddleName.trim()) || c.MiddleName.toUpperCase().trim().equals(c.MiddleName.trim()))) {
          c.MiddleName = Report_Custom_Utils.toProperCase(c.MiddleName);
        }
        if (c.FirstName != null && c.FirstName.length() > 2 && (c.FirstName.toLowerCase().trim().equals(c.FirstName.trim()) || c.FirstName.toUpperCase().trim().equals(c.FirstName.trim()))) {
          c.FirstName= Report_Custom_Utils.toProperCase(c.FirstName);            
        }
        if (c.Shift__c == null || c.Shift__c == '') {
          c.Shift__c = 'Unknown';
        }
        if (c.Department == null || c.Department == '') {
          c.Department = 'Unknown';
        }
        if (c.Worksite__c == null || c.Worksite__c == '') {
          c.Worksite__c = 'Unknown';
        }                
      }
      System.debug('[toProperCase] finish');
    }

   
