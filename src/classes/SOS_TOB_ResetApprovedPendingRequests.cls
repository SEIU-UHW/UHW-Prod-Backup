public class SOS_TOB_ResetApprovedPendingRequests{

    // This Class being used to Reset the Approved and Pending Values in the Time off Balance Object  (SOS_Time_Off_Balance__c)
    // Resets the following fields
    

     public void Reset()
     {
     
             try
             {
        
              List<SOS_Time_Off_Balance__c> tobRecords = [SELECT Bereavement_Time_Approved__c, Bereavement_Time_Pending__c,
                 Birthday_Time_Approved__c, Birthday_Time_Pending__c,
                 Float_Time_Approved__c, Float_Time_Pending__c,
                 Holiday_work_days_Time_Approved__c, Holiday_work_days_Time_Pending__c,
                 Jury_Duty_Time_Approved__c, Jury_Duty_Time_Pending__c,
                 Relieve_Time_Approved__c, Relieve_Time_Pending__c, 
                 Religious_OPEIU_Only_Time_Approved__c, Religious_OPEIU_Only_Time_Pending__c,
                 Sick_Time_Approved__c, Sick_Time_Pending__c,
                 Unpaid_Time_Approved__c, Unpaid_Time_Pending__c,
                 Vacation_Time_Approved__c, Vacation_Time_Pending__c
            FROM  SOS_Time_Off_Balance__c
            WHERE Owner.IsActive = TRUE  LIMIT 4000];
            
            for (SOS_Time_Off_Balance__c tobRec: tobRecords) 
            {
                tobRec.Bereavement_Time_Approved__c = 0;
                tobRec.Bereavement_Time_Pending__c = 0;
                tobRec.Birthday_Time_Approved__c = 0;
                tobRec.Birthday_Time_Pending__c = 0;
                tobRec.Float_Time_Approved__c = 0;
                tobRec.Float_Time_Pending__c = 0;
                tobRec.Holiday_work_days_Time_Approved__c = 0;
                tobRec.Holiday_work_days_Time_Pending__c = 0;
                tobRec.Jury_Duty_Time_Approved__c = 0;
                tobRec.Jury_Duty_Time_Pending__c = 0;
                tobRec.Relieve_Time_Approved__c = 0;
                tobRec.Relieve_Time_Pending__c = 0; 
                tobRec.Religious_OPEIU_Only_Time_Approved__c = 0;
                tobRec.Religious_OPEIU_Only_Time_Pending__c = 0;
                tobRec.Sick_Time_Approved__c = 0;
                tobRec.Sick_Time_Pending__c = 0;
                tobRec.Unpaid_Time_Approved__c = 0;
                tobRec.Unpaid_Time_Pending__c = 0;
                tobRec.Vacation_Time_Approved__c = 0;
                tobRec.Vacation_Time_Pending__c = 0;
                
            }
            
            
                update tobRecords;
                ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.CONFIRM, 'Approved/Pending Requests hours have been Reset.'));
         
            }
            catch( Exception ex)
            {
                ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, 'Error:' + ex));
            
            }
         
     }
     
