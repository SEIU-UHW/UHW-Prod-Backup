        </actions>
        <actions>
            <name>Set_Pardot_Hard_Bounced_False</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND (2 OR 3 OR 4)</booleanFilter>
        <criteriaItems>
            <field>Contact.EBoard__c</field>
            <operation>equals</operation>
            <value>True</value>
