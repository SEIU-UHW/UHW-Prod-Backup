    <rules>
        <fullName>Clears Medical Note Expiration Date</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Contact.Division_Type__c</field>
            <operation>equals</operation>
            <value>Hospital,Kaiser,Staff</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Medical_Note_Expiration_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Clears value for Medical Note Expiration Date when expired.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Update_Medical_Note_Expiration_Date</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Contact.Medical_Note_Expiration_Date__c</offsetFromField>
            <timeLength>-3</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
