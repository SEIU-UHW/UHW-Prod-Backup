    <rules>
        <fullName>Removes Do No Email %26 Opted Out - E Board Members</fullName>
        <actions>
            <name>Set_DNE_False</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Set_Email_Opt_Out_False</name>
